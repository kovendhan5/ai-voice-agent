# Environment variables for Orpheus Voice Chat
# Copy this file to .env and fill in your values

# Server Configuration
PORT=8080
HOST=0.0.0.0
DEBUG=false

# Orpheus TTS Configuration
ORPHEUS_MODEL_NAME=canopylabs/orpheus-tts-0.1-finetune-prod
DISABLE_TRANSFORMERS=true

# Audio Configuration
SAMPLE_RATE=24000
AUDIO_FORMAT=wav

# Cache Configuration
MODEL_CACHE_DIR=.cache/models

# Logging
LOG_LEVEL=INFO
